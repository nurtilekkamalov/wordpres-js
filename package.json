{
  "name": "wordpres-js",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "npm run generate && concurrently \"npx faust dev\" \"npm run watch-codegen\"",
    "build": "npm run generate && npx faust build",
    "generate": "npx faust generatePossibleTypes && graphql-codegen",
    "start": "npx faust start",
    "codegen": "graphql-codegen",
    "watch-codegen": "graphql-codegen -w",
    "wc": "graphql-codegen -w",
    "tsc": "tsc"
  },
  "dependencies": {
    "next": "14.0.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "@faustwp/next": "0.15.14",
    "@faustwp/react": "0.15.13",
    "@faustwp/core": "0.15.13"
  },
  "devDependencies": {
    "@faustwp/cli": "0.15.3",
    "@graphql-codegen/cli": "^3.0.0",
    "concurrently": "^8.2.0",
    "graphql": "^16.8.1",
    "typescript": "^5.3.3"
  }
}
